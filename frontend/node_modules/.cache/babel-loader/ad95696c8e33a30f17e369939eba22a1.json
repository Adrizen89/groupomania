,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,yBAAA;AAAA,OADA,EAEA,UAAA,CAAA;AAAA,eAAA,YAAA,IAAA,CAAA,CAAA,KAAA,+BAAA;AAAA,OAFA,CAVA;AAcA,MAAA,SAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,+BAAA;AAAA,OADA,EAEA,UAAA,CAAA;AAAA,eAAA,CAAA,IAAA,CAAA,CAAA,MAAA,IAAA,CAAA,IAAA,sBAAA;AAAA,OAFA,EAGA,UAAA,CAAA;AAAA,eAAA,cAAA,IAAA,CAAA,CAAA,KAAA,wBAAA;AAAA,OAHA,EAIA,UAAA,CAAA;AAAA,eAAA,WAAA,IAAA,CAAA,CAAA,KAAA,qBAAA;AAAA,OAJA,CAdA;AAoBA,MAAA,UAAA,EAAA;AACA,QAAA,SAAA,EAAA,EADA;AAEA,QAAA,QAAA,EAAA,EAFA;AAGA,QAAA,KAAA,EAAA,EAHA;AAIA,QAAA,QAAA,EAAA;AAJA,OApBA;AA0BA,MAAA,WAAA,EAAA,EA1BA;AA2BA,MAAA,IAAA,EAAA,IA3BA;AA4BA,MAAA,GAAA,EAAA,KA5BA;AA6BA,MAAA,OAAA,EAAA,EA7BA;AA+BA,MAAA,MAAA,EAAA;AA/BA,KAAA;AAiCA,GApCA;AAqCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AAAA;;AACA,WAAA,WAAA,GAAA,IAAA,CAAA,SAAA,CAAA,KAAA,UAAA,CAAA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,uCAAA,EAAA,KAAA,WAAA,EAAA;AAAA,QAAA,OAAA,EAAA;AAAA,0BAAA;AAAA;AAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,YAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,IAAA,CAAA,OAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,KAAA;AACA,QAAA,KAAA,CAAA,GAAA,GAAA,IAAA;AACA,OANA,WAOA,UAAA,KAAA,EAAA;AAEA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,QAAA,KAAA,CAAA,GAAA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAZA;AAaA;AAhBA;AArCA,CAAA","sourcesContent":["<template>\r\n    <v-app class=\"signup ma-auto mt-6\" v-if=\"form\">\r\n        <v-card class=\"signup__card\" raised>\r\n            <v-card-text>\r\n                <v-form ref=\"form\" v-model=\"valid\">\r\n                    <v-text-field  v-model=\"dataSignup.firstName\" :rules=\"firstNameRules\" label=\"Prénom\" prepend-icon=\"mdi-account-outline\" color=\"black\" autofocus required></v-text-field>\r\n                    <v-text-field  v-model=\"dataSignup.lastName\" :rules=\"lastNameRules\" label=\"Nom\" prepend-icon=\"mdi-account-outline\" color=\"black\" required></v-text-field>\r\n                    <v-text-field  v-model=\"dataSignup.email\" :rules=\"emailRules\" label=\"e-mail\" prepend-icon=\"mdi-at\" color=\"black\" required></v-text-field>\r\n                    <v-text-field  v-model=\"dataSignup.password\" :rules=\"passRules\" error-count=\"4\" type=\"password\" label=\"Mot de passe\" prepend-icon=\"mdi-lock-outline\" color=\"black\" required></v-text-field>\r\n                </v-form>\r\n                <template>\r\n                  <div class=\"text-center\">\r\n                    <v-dialog v-model=\"dialog\" persistent width=\"500\">\r\n                      <template v-slot:activator=\"{ on, attrs }\">\r\n                        <v-btn :disabled=\"!valid\" class=\"success\" v-bind=\"attrs\" v-on=\"on\">Confirmer</v-btn>\r\n                      </template>\r\n                      <v-card>\r\n                        <v-card-title class=\"text-center\">\r\n                          Votre compte a été créé ! <br/>\r\n                          Veuillez vous connecter pour accéder à l'intranet.\r\n                        </v-card-title>\r\n                        <v-divider></v-divider>\r\n                        <v-card-actions>\r\n                          <v-spacer></v-spacer>\r\n                          <v-btn @click=\"sendSignup()\" class=\"success\">Se connecter</v-btn>\r\n                        </v-card-actions>\r\n                      </v-card>\r\n                    </v-dialog>\r\n                  </div>\r\n                </template>\r\n                <p v-if=\"msg\">{{ message }}</p>\r\n            </v-card-text>\r\n        </v-card>\r\n    </v-app>\r\n</template>\r\n<script>\r\nimport axios from \"axios\"\r\n\r\nexport default {\r\n    name : \"Signup\",\r\n    data(){\r\n        return{\r\n            valid: true,\r\n            firstNameRules: [\r\n                v => !!v || 'Renseignez votre prénom',\r\n                v => /^[A-Za-z]+$/.test(v) || \"Votre prénom n'est pas valide\",\r\n            ],\r\n            lastNameRules: [\r\n                v => !!v || 'Renseignez votre nom',\r\n                v => /^[A-Za-z]+$/.test(v) || \"Votre nom n'est pas valide\",\r\n            ],\r\n            emailRules: [\r\n                v => !!v || 'Renseignez votre e-mail',\r\n                v => /.+@.+\\..+/.test(v) || \"Votre e-mail n'est pas valide\",\r\n            ],\r\n            passRules: [\r\n                v => !!v || 'Renseignez votre mot de passe',\r\n                v => (v && v.length >= 5) || '5 caractères minimun',\r\n                v => /(?=.*[A-Z])/.test(v) || 'Au moins une majuscule', \r\n                v => /(?=.*\\d)/.test(v) || 'Au moins un chiffre',\r\n            ],\r\n            dataSignup:{\r\n                firstName: \"\",\r\n                lastName: \"\",\r\n                email: \"\",\r\n                password: \"\",\r\n            },\r\n            dataSignupS: \"\", \r\n            form: true,\r\n            msg: false,\r\n            message: \"\",\r\n\r\n            dialog:\"\"\r\n        }\r\n    },\r\n    methods: {\r\n        sendSignup(){\r\n            this.dataSignupS = JSON.stringify(this.dataSignup)\r\n            axios.post('http://localhost:3000/api/auth/signup', this.dataSignupS, {headers: {'Content-Type': 'application/json'}})\r\n            .then(response => {\r\n                let sign = JSON.parse(response.data);\r\n                this.message = sign.message;\r\n                this.form = false;\r\n                this.msg = true;\r\n            })\r\n            .catch(error => {\r\n                \r\n                this.message = error;\r\n                this.msg = true; \r\n                console.log(error);\r\n                });\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n    .signup{\r\n        position: flex;\r\n        flex-direction: row;\r\n        top: 5%;\r\n        width: 350px;\r\n        height: 200px;\r\n        text-align: center;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components/Auth"}]}